{
    "name": "opis-colibri/html",
    "type": "opis-colibri-module",
    "description": "HTML module",
    "keywords": ["opis", "colibri", "module", "html"],
    "homepage": "http://opis.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        }
    ],
    "require": {
        "opis/colibri": "2.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Opis\\Colibri\\Modules\\Html\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\Colibri\\Modules\\Html\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "module": {
            "title": "HTML",
            "collector": "Opis\\Colibri\\Modules\\Html\\Collector"
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
